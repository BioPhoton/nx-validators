{
    "generators": {
        "check-version-mismatch": {
            "factory": "./src/workspace-validation/validators/check-version-mismatch/generator",
            "schema": "./src/workspace-validation/validators/check-version-mismatch/schema.json",
            "description": "Check if the versions in the package.json are valid"
        },
        "check-eslint-config": {
            "factory": "./src/workspace-validation/validators/check-eslint-config/generator",
            "schema": "./src/workspace-validation/validators/check-eslint-config/schema.json",
            "description": "Check if eslint is correctly configured"
        },
        "use-project-json": {
            "factory": "./src/workspace-validation/validators/use-project-json/generator",
            "schema": "./src/workspace-validation/validators/use-project-json/schema.json",
            "description": "Check if the project.json file is configured"
        },
        "use-nx-cloud": {
            "factory": "./src/workspace-validation/validators/use-nx-cloud/generator",
            "schema": "./src/workspace-validation/validators/use-nx-cloud/schema.json",
            "description": "Check if Nx Cloud is configured"
        },
        "check-prettier-config": {
            "factory": "./src/workspace-validation/validators/check-eslint-config/generator",
            "schema": "./src/workspace-validation/validators/check-eslint-config/schema.json",
            "description": "Check if prettier is correctly configured"
        },
        "check-root-tsconfig-base": {
            "factory": "./src/workspace-validation/validators/check-root-tsconfig-base/generator",
            "schema": "./src/workspace-validation/validators/check-root-tsconfig-base/schema.json",
            "description": "Check if root tsconfig.base.json is correctly configured"
        },
        "check-tsconfig-paths": {
            "factory": "./src/workspace-validation/validators/check-tsconfig-paths/generator",
            "schema": "./src/workspace-validation/validators/check-tsconfig-paths/schema.json",
            "description": "Check if tsconfig paths are correctly configured"
        },
        "validate-workspace": {
            "factory": "./src/workspace-validation/validate-workspace/generator",
            "schema": "./src/workspace-validation/validate-workspace/schema.json",
            "description": "Validation of the entire workspace"
        },
        "check-gulp-usage": {
            "factory": "./src/workspace-validation/validators/check-gulp-usage/generator",
            "schema": "./src/workspace-validation/validators/check-gulp-usage/schema.json",
            "description": "check-gulp-usage generator"
        }
    }
}
